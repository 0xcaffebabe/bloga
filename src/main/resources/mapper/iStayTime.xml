<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wang.ismy.bloga.dao.inter.IStayTime">

    <!--更新访客留存时间-->
    <update id="updateStatus" parameterType="int">
        UPDATE stay_time set residence_time=residence_time+10 WHERE id=#{id}
    </update>
</mapper>